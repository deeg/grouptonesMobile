        <div id="headerWrapper" data-role="header">
            <a href="#sidePanel" data-role="button" data-inline="true" data-icon="grid"></a>
            <p id="logo">Grouptones</p>
        </div>
        <div data-role="content">
            {+dynamicBody}
            {/dynamicBody}
        </div>

        <div data-role="panel" data-display="overlay" id="sidePanel">
            {?currentUser.artist_name}
            <p> <a href="/"> <img src="/img/home.png"></a> </p>
            <p> <a href="/faq"><img src="/img/faq.png"></a> </p>
            <p> <a href="/contact"><img src="/img/contactUs.png"></a> </p>
            <p><a href="/logout"><img src="/img/logout.png"></a> </p>
            {:else}

            <p><a href="/register" data-role="button" data-theme="b">Register</a> </p>
            <p><a href="/login" data-role="button" data-theme="b">Login</a> </p>
            <p><a href="/faq" data-role="button" data-theme="b">FAQ</a></p>
            <p><a href="/contact" data-role="button" data-theme="b">Contact Us</a> </p>
            {/currentUser.artist_name}
            <!-- panel content goes here -->
        </div>
    </div>
